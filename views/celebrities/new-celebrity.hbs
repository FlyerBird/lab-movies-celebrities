<h2>Create a Celebrity</h2>

  <form method="POST" action="/celebrities/create">

      <label>Add name</label>
      <input type="string" name="name"/> 

      <label>Add Occupation</label>
      <input type="string" name="occupation"/>

      <label>Add Catch Catch Phrase</label>
      <input type="string" name="catchPhrase"/>

      <button type="submit">Create</button>
    </form>